initially(pname, fdirectory)
   projectname := pname
   filedirectory := fdirectory

   cc := "unicon"    #linux compiler
   ld := "unicon"    #linux linker
   wcc := "wicont"   #windows compiler
   wld := "wicont"   #windows compiler
   cFlags := "-c"
   lFlags := ""

   self.Dialog.initially()
end

