    DisplayLine("UTF8.LexEquals(us1, s2)", (UTF8.LexEquals(us1, s2) & "Succeeded") | "Failed")
    DisplayLine("UTF8.LexEquals(us1, us2)", (UTF8.LexEquals(us1, us2) & "Succeeded") | "Failed")
