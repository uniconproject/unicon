####################################################################
#
# Make entries for the compiler library
#

comp_all:
	cd ..\common
	$(MAKE) $(ICOBJS) dlrgint.$(O)
	cd ..\runtime
	$(MAKE) db_lib

comp_all_uniconc:
	cd ..\common
	$(MAKE) $(ICOBJS) dlrgint.$(O)
	cd ..\runtime
	$(MAKE) db_lib_uniconc

db_lib: rt.db rt.lib
db_lib_uniconc: rt.db.uniconc rt.lib

#
# if rt.db is missing or any header files have been updated, recreate
# rt.db from scratch along with the .$(O) files.
#
rt.db: $(HDRS)
	$(RM) -f rt.db rt.lib
	$(RTT) cnv.r data.r def.r errmsg.r fconv.r fmath.r\
	  fmisc.r fmonitr.r fscan.r fstr.r fstranl.r fstruct.r\
	  fsys.r fwindow.r init.r invoke.r keyword.r\
	  lmisc.r oarith.r oasgn.r ocat.r ocomp.r omisc.r\
	  oref.r oset.r ovalue.r ralc.r rcoexpr.r rcomp.r rmsg.r \
	  rdebug.r rlrgint.r rlocal.r rmemmgt.r rmisc.r rstruct.r\
	  rsys.r rgfxsys.r rwinsys.r rwindow.r rposix.r fxtra.r
	$(CC) $(CFLAGS) -c @rttcur.lnk
	$(RM) *.c

rt.db.uniconc: $(HDRS)
	$(RM) -f rt.db rt.lib
	$(RTT) -DUniconc cnv.r data.r def.r errmsg.r fconv.r fmath.r\
	  fmisc.r fmonitr.r fscan.r fstr.r fstranl.r fstruct.r\
	  fsys.r fwindow.r init.r invoke.r keyword.r\
	  lmisc.r oarith.r oasgn.r ocat.r ocomp.r omisc.r\
	  oref.r oset.r ovalue.r ralc.r rcoexpr.r rcomp.r rmsg.r\
	  rdebug.r rlrgint.r rlocal.r rmemmgt.r rmisc.r rstruct.r\
	  rsys.r rgfxsys.r rwinsys.r rwindow.r rposix.r fxtra.r
	$(CC) $(CFLAGS) -DUniconc -c @rttcur.lnk
	$(RM) *.c


rt.lib: ../common/rswitch.$(O) ../common/long.$(O) ../common/time.$(O) \
      cnv.$(O) data.$(O) def.$(O) errmsg.$(O) fconv.$(O) fmath.$(O) fmisc.$(O) fmonitr.$(O) fscan.$(O)\
      fstr.$(O) fstranl.$(O) fstruct.$(O) fsys.$(O) fwindow.$(O) init.$(O) invoke.$(O)\
      keyword.$(O) lmisc.$(O) oarith.$(O) oasgn.$(O) ocat.$(O) ocomp.$(O) omisc.$(O) oref.$(O) oset.$(O)\
      ovalue.$(O) ralc.$(O) rcoexpr.$(O) rcomp.$(O) rmsg.$(O) rdebug.$(O) rlrgint.$(O) rlocal.$(O) rmemmgt.$(O)\
      rmisc.$(O) rstruct.$(O) rsys.$(O) rgfxsys.$(O) rwinsys.$(O) fxtra.$(O)\
      rposix.$(O) rwindow.$(O) ../common/xwindow.$(O)
	$(RM) -f rt.lib
	lib /OUT:rt.lib @rttfull.lnk ../common/rswitch.$(O)\
	    ../common/long.$(O) ../common/time.$(O) \
	    ../common/xwindow.$(O) ../common/alloc.$(O)
	cp rt.lib rt.db ../common/dlrgint.$(O) ../../bin

