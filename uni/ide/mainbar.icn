   method save_config()
   local f
   if f := open(getenv("HOME"|"HOMEDIR")||PS||".uirc","w") then {
      write(f,	#"width=",width,"\n",
		#"height=",height,"\n",
		#"msglines=",msglines,"\n",
		"font=",fontname, "\n",
		"fg=",fgcolor #, "\n",
		#"linebreak=",linebreak
		)
      }
   else {
      MsgBox.set_contents(["couldn't save options"])
      }
   end

   method load_config()
   local f
   if f := open(getenv("HOME"|"HOMEDIR")||PS||".uirc","r") then {
      while line := read(f) do {
	 line ? {
	    if ="msglines" then {
	       tab(upto(&digits))
	       msglines := integer(tab(many(&digits)))
	       }
	    else if ="height" then {
	       tab(upto(&digits))
	       height := integer(tab(many(&digits)))
	       }
	    else if ="width" then {
	       tab(upto(&digits))
	       width := integer(tab(many(&digits)))
	       }
	    else if ="font" then {
	       tab(many('= \t'))
	       fontname := tab(0)
	       }
	    else if ="fg" then {
		tab(many('= \t'))
		fgcolor := tab(0)
	    }
	    else if ="linebreak" then {
	       tab(many('= \t'))
	       linebreak := tab(0)
	       }
	}
     }
   }
   end

