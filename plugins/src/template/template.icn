class Template : uso::USO()
   # return 0
   # @i
   method foo(value:integer)
      static F
      initial F:=loadfunc("libtemplate.so", "foo")

      return F(value)
   end
   method api()
      return [
              lang::find_method(self, "foo"),
              ]
   end
end

